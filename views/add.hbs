        <input id="title" name="title" type="text" class="validate" required>
        <label for="title">Name food</label>
        <span class="helper-text" data-error="Enter name"></span>
    </div>
    <div class="input-field">
        <input id="price" name="price" type="text" class="validate" required>
        <label for="price">Price</label>
        <span class="helper-text" data-error="Enter price"></span>
    </div>
    <div class="input-field">
        <input id="image" name="image" type="text" class="validate" required>
